package net.believecollective.ag2010.view.factory {	import net.believecollective.ag2010.view.controls.ControlsView;	import net.believecollective.ag2010.view.eventsdetail.EventsDetailView;	import net.believecollective.ag2010.view.feedback.FeedbackView;	import net.believecollective.ag2010.view.help.HelpView;	import net.believecollective.ag2010.view.loader.LoaderView;	import net.believecollective.ag2010.view.navigator.NavigatorView;	import net.believecollective.ag2010.view.testpanel.TestPanelView;	import net.believecollective.ag2010.view.timeline.TimelineView;	import net.believecollective.ag2010.view.tooltip.AGToolTipView;	import net.believecollective.frameworks.pureMVC2.view.ViewBase;	import net.believecollective.utils.Tracer;	/**	 * @author Dan Mackie (dan@believecollective.net)	 */	public class ViewFactory {				public static function CREATE(arg_s : String) : ViewBase {			switch (arg_s) {				case TimelineView.NAME:					return new TimelineView();					break;				case NavigatorView.NAME:					return new NavigatorView();					break;				case EventsDetailView.NAME:					return new EventsDetailView();					break;				case FeedbackView.NAME:					return new FeedbackView();					break;				case AGToolTipView.NAME:					return new AGToolTipView();					break;				case TestPanelView.NAME:					return new TestPanelView();					break;				case LoaderView.NAME:					return new LoaderView();					break;				case ControlsView.NAME:					return new ControlsView();					break;				case HelpView.NAME:					return new HelpView();					break;				default:					Tracer.TRACE("!!!Dan M : ViewFactory.create : NEED TO THE VIEW FACTORY LIST: "+arg_s);			}			return null;		}	}}