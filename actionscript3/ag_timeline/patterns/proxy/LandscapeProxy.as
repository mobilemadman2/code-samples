package net.believecollective.ag2010.patterns.proxy {	import net.believecollective.ag2010.helpers.AGMethodHelper;	import net.believecollective.ag2010.model.vo.timeline.AGLandscapeVO;	import net.believecollective.ag2010.model.vo.timeline.AGTimelineVO;	import org.puremvc.as3.interfaces.IProxy;	import org.puremvc.as3.patterns.proxy.Proxy;	/**	 * @author Dan Mackie (dan@believecollective.net)	 */	public class LandscapeProxy extends Proxy implements IProxy {		public static const NAME : String = "LandscapeProxy";				private var _oLandscapeVO : AGLandscapeVO;				public function LandscapeProxy() {			super(NAME);		}				public function set landscape_xml(xml : XML) : void {			_oLandscapeVO = AGMethodHelper.parseLandscape(															xml, 															facade.retrieveProxy(HistoryProxy.NAME) as HistoryProxy, 															(facade.retrieveProxy(ConfigProxy.NAME) as ConfigProxy).config.eraDateData															);		}		public function get landscape() : AGTimelineVO {			return _oLandscapeVO;		}			}}