package net.believecollective.ag2010.patterns.commands {	import net.believecollective.ag2010.app.ApplicationFacade;	import net.believecollective.ag2010.patterns.proxy.ApplicationProxy;	import net.believecollective.ag2010.patterns.proxy.TimelineProxy;	import org.puremvc.as3.interfaces.ICommand;	import org.puremvc.as3.interfaces.INotification;	import org.puremvc.as3.patterns.command.SimpleCommand;	/**		 * @author Dan Mackie (dan@believecollective.net)	 */	public class ParseXMLTimelineCommand extends SimpleCommand implements ICommand {		override public function execute(notification : INotification) : void {			var t_XML : XML = notification.getBody().xml;			var t_oTP : TimelineProxy = facade.retrieveProxy(TimelineProxy.NAME) as TimelineProxy;			t_oTP.timeline_xml = t_XML;			var t_oAP : ApplicationProxy = facade.retrieveProxy(ApplicationProxy.NAME) as ApplicationProxy;			t_oAP.fTimelineParsed();						//TODO: SEND INIT NOTE HERE			sendNotification(ApplicationFacade.CMD_START_INTERFACE);		}
	}	}