package net.believecollective.ag2010.patterns.commands {	import net.believecollective.ag2010.patterns.proxy.ConfigProxy;	import org.puremvc.as3.interfaces.ICommand;	import org.puremvc.as3.interfaces.INotification;	import org.puremvc.as3.patterns.command.SimpleCommand;	/**	 * @author Dan Mackie (dan@believecollective.net)	 */	public class ParseEraXMLCommand extends SimpleCommand implements ICommand {		override public function execute(notification : INotification) : void {			var t_XML : XML = notification.getBody().xml;			var t_oCP : ConfigProxy = facade.retrieveProxy(ConfigProxy.NAME) as ConfigProxy;			t_oCP.fAddDateData(t_XML);		}
	}}