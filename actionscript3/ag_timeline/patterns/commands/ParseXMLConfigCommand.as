package net.believecollective.ag2010.patterns.commands {	import net.believecollective.ag2010.app.ApplicationFacade;	import net.believecollective.ag2010.patterns.proxy.ConfigProxy;	import org.puremvc.as3.interfaces.ICommand;	import org.puremvc.as3.interfaces.INotification;	import org.puremvc.as3.patterns.command.SimpleCommand;	/**		 * @author Dan Mackie (dan@believecollective.net)	 */	public class ParseXMLConfigCommand extends SimpleCommand implements ICommand {		override public function execute(notification : INotification) : void {			var t_XML : XML = notification.getBody().xml;			var t_oCP : ConfigProxy = facade.retrieveProxy(ConfigProxy.NAME) as ConfigProxy;			t_oCP.config_xml = t_XML;			//			if (ApplicationFacade.LANGUAGE == "ne"){				sendNotification(ApplicationFacade.CMD_LOAD_FONTS, [ApplicationFacade.FONT_GURUMAA]);			}else{				sendNotification(ApplicationFacade.CMD_LOAD_FONTS, [					ApplicationFacade.FONT_CORBEL_REGULAR,					ApplicationFacade.FONT_CORBEL_BOLD,					ApplicationFacade.FONT_AUDIMAT_ALL]);			}		}
	}}