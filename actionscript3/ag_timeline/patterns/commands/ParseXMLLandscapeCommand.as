package net.believecollective.ag2010.patterns.commands {	import net.believecollective.ag2010.app.ApplicationFacade;	import net.believecollective.ag2010.patterns.proxy.LandscapeProxy;	import org.puremvc.as3.interfaces.ICommand;	import org.puremvc.as3.interfaces.INotification;	import org.puremvc.as3.patterns.command.SimpleCommand;	/**		 * @author Dan Mackie (dan@believecollective.net)	 */	public class ParseXMLLandscapeCommand extends SimpleCommand implements ICommand {		override public function execute(notification : INotification) : void {			var t_XML : XML = notification.getBody().xml;			var t_oTP : LandscapeProxy = facade.retrieveProxy(LandscapeProxy.NAME) as LandscapeProxy;			t_oTP.landscape_xml = t_XML;			//TODO: SEND INIT NOTE HERE			sendNotification(ApplicationFacade.CMD_START_INTERFACE_BG);		}
	}	}