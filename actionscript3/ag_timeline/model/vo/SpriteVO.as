package net.believecollective.ag2010.model.vo {	import net.believecollective.model.SimpleConfig;		/**	 * @author Dan Mackie (dan@believecollective.net)	 */	public class SpriteVO extends SimpleConfig {		private var _aXY : Array;		private var _aWH : Array;				public function SpriteVO(arg_aXY:Array, arg_aWH:Array) {			_aXY = arg_aXY;			_aWH = arg_aWH;		}		public function get coordinates() : Array {			return _aXY;		}		public function set coordinates(arg_a : Array) : void {			_aXY = arg_a;		}				public function get dimensions() : Array {			return _aWH;		}		public function set dimensions(arg_a : Array) : void {			_aWH = arg_a;		}			}}